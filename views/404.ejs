                            <ul class="error__list">
                                <li>
                                    <a href="#" class="link">Go back to previous page</a>
                                </li>
                                <li>
                                    <a href="index.ejs" class="link">Go to homepage</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <!-- END content-->
            </div>
            <!-- Footer -->
            <% include footer.ejs %>
        <!-- END Footer -->
        <!-- All JavaScript libraries -->
